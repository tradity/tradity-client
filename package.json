{
  "name": "tradity-client",
  "version": "0.7.0",
  "engines": {
    "node": ">=5.0.0"
  },
  "license": "MPL-2.0",
  "private": true,
  "repository": "tradity/tradity-client",
  "bugs": {
    "url": "https://github.com/tradity/tradity-client/issues"
  },
  "scripts": {
    "lint": "jshint --config=.jshintrc --reporter=node_modules/jshint-stylish app/js/*.js app/js/*/*.js",
    "test": "npm run lint",
    "i18n": "ng-xi18n",
    "doc": "compodoc -p tsconfig.json -d docs",
    "clean": "rimraf dist/ aot/ tmp/ docs/ && mkdir -p dist/",
    "prebuild": "npm run clean",
    "build": "parallelshell 'npm run build:js' 'npm run build:cp' 'tsc -p tsconfig.json' 'npm run build:indexhtml -- -e dev'",
    "prebuild:prod": "npm run clean",
    "build:prod": "npm run build:cp && npm run build:indexhtml -- -e prod && ngc -p tsconfig.prod.json && rollup -c rollup-config.js",
    "build:indexhtml": "htmlprocessor index.html -o index.dev.html",
    "build:js": "cp -r app dist/",
    "build:cp": "cp -r *.txt *.ico manifest.json fonts img scripts apidoc dist/",
    "start": "npm run build:indexhtml -- -e dev && parallelshell 'tsc -p tsconfig.json -w' 'es-dev-server --app-index index.dev.html --node-resolve --watch'"
  },
  "dependencies": {
    "@angular/animations": "^6.1.2",
    "@angular/common": "^6.1.2",
    "@angular/compiler": "^6.1.2",
    "@angular/compiler-cli": "^6.1.2",
    "@angular/core": "^6.1.2",
    "@angular/forms": "^6.1.2",
    "@angular/platform-browser": "^6.1.2",
    "@angular/platform-browser-dynamic": "^6.1.2",
    "@angular/platform-server": "^6.1.2",
    "@angular/router": "^6.1.2",
    "@ngrx/effects": "^6.1.0",
    "@ngrx/store": "^6.1.0",
    "chart.js": "^2.7.1",
    "core-js": "^2.5.7",
    "lit-html": "^1.2.1",
    "material-design-icons": "^3.0.1",
    "normalize.css": "^7.0.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.2.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@compodoc/compodoc": "1.0.1",
    "@types/chart.js": "^2.7.3",
    "@types/node": "10.x.x",
    "angular": "^1.6.6",
    "angular-marked": "^1.2.2",
    "es-dev-server": "^1.56.0",
    "hashmark": "^5.0.0",
    "htmlprocessor": "^0.2.6",
    "jshint": "^2.9.5",
    "jshint-stylish": "2.2.1",
    "marked": "^0.4.0",
    "parallelshell": "^3.0.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.64.1",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-commonjs": "^9.1.5",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify": "^4.0.0",
    "rollup-pluginutils": "^2.3.1",
    "typescript": "^3.8.3"
  }
}
