{
  "name": "tradity-client",
  "version": "0.7.0",
  "engines": {
    "node": ">=5.0.0"
  },
  "license": "MPL-2.0",
  "private": true,
  "repository": "tradity/tradity-client",
  "bugs": {
    "url": "https://github.com/tradity/tradity-client/issues"
  },
  "scripts": {
    "lint": "jshint --config=.jshintrc --reporter=node_modules/jshint-stylish app/js/*.js app/js/*/*.js",
    "test": "npm run lint",
    "i18n": "ng-xi18n",
    "doc": "compodoc -p tsconfig.json -d docs",
    "clean": "rimraf dist/ aot/ tmp/ docs/ && mkdir -p dist/",
    "prebuild": "npm run clean",
    "build": "npm run build:cp && htmlprocessor index.html -o dist/index.html -e prod && ngc -p tsconfig.prod.json && rollup -c rollup-config.js",
    "build:cp": "cp -r *.txt *.ico manifest.json fonts img scripts apidoc dist/",
    "start": "htmlprocessor index.html -o index.dev.html -e dev && parallelshell 'tsc -p tsconfig.json -w' 'es-dev-server --app-index index.dev.html --node-resolve --watch --compatibility none'"
  },
  "dependencies": {
    "@angular/common": "8.x.x",
    "@angular/compiler": "8.x.x",
    "@angular/compiler-cli": "8.x.x",
    "@angular/core": "8.x.x",
    "@angular/forms": "8.x.x",
    "@angular/platform-browser": "8.x.x",
    "@angular/platform-browser-dynamic": "8.x.x",
    "@angular/platform-server": "8.x.x",
    "@angular/router": "8.x.x",
    "@ngrx/effects": "8.x.x",
    "@ngrx/store": "8.x.x",
    "chart.js": "^2.7.1",
    "lit-html": "^1.2.1",
    "material-design-icons": "^3.0.1",
    "normalize.css": "^7.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.4.0",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@compodoc/compodoc": "1.0.1",
    "@types/chart.js": "^2.7.3",
    "@types/node": "10.x.x",
    "angular": "^1.6.6",
    "angular-marked": "^1.2.2",
    "es-dev-server": "^1.56.0",
    "hashmark": "^5.0.0",
    "htmlprocessor": "^0.2.6",
    "jshint": "^2.9.5",
    "jshint-stylish": "2.2.1",
    "marked": "^0.4.0",
    "parallelshell": "^3.0.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.64.1",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify": "^4.0.0",
    "rollup-pluginutils": "^2.3.1",
    "typescript": "^3.8.3"
  }
}
