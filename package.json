{
  "name": "tradity-client",
  "version": "0.7.0",
  "devDependencies": {
    "@types/core-js": "^0.9.33",
    "angular-write-constants": "^1.0.2",
    "autoprefixer": "^6.3.1",
    "browser-sync": "^2.17.3",
    "connect-history-api-fallback": "^1.1.0",
    "cssmin": "^0.4.3",
    "hashmark": "^3.1.0",
    "jshint": "^2.8.0",
    "jshint-stylish": "~0.1.3",
    "less": "^2.5.3",
    "ngdoc-md": "^1.0.3",
    "parallelshell": "^2.0.0",
    "postcss-cli": "^2.4.1",
    "requirejs": "^2.1.11",
    "rimraf": "^2.5.1",
    "typescript": "^2.0.2",
    "watch-run": "^1.2.4"
  },
  "engines": {
    "node": ">=5.0.0"
  },
  "scripts": {
    "lint": "jshint --config=.jshintrc --reporter=node_modules/jshint-stylish app/js/*.js app/js/*/*.js",
    "test": "npm run lint",
    "clean": "rimraf dist/ docs/ && mkdir -p docs/ dist/app/js/jit/l10n/",
    "prebuild": "npm run clean",
    "build": "parallelshell 'npm run build:js' 'npm run build:cp'",
    "build:js": "cp -r app dist/ && (cd dist && ln -sf ../node_modules || true)",
    "build:cp": "cp -r *.html *.txt *.ico fonts img scripts apidoc dist/",
    "watch": "parallelshell 'npm run watch:js' 'npm run watch:cp'",
    "watch:js": "watch-run -p 'app/**' npm run build:js",
    "watch:cp": "watch-run -p '*.html,*.txt,*.ico,fonts/**,img/**,scripts/**' npm run build:cp",
    "start": "npm run build && parallelshell 'npm run watch' 'browser-sync start --config bs-config.js'",
    "tsc": "tsc -p . -w"
  },
  "dependencies": {
    "@angular/common": "2.1.x",
    "@angular/compiler": "2.1.x",
    "@angular/core": "2.1.x",
    "@angular/forms": "2.1.x",
    "@angular/http": "2.1.x",
    "@angular/platform-browser": "2.1.x",
    "@angular/platform-browser-dynamic": "2.1.x",
    "@angular/router": "3.1.x",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "^0.19.39",
    "zone.js": "^0.6.25",
    "normalize.css": "^4.2.0"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/tradity/tradity-client.git"
  },
  "bugs": {
    "url": "https://github.com/tradity/tradity-client/issues"
  }
}
